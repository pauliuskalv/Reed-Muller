<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity"
      maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity"
      prefHeight="120.0"
      prefWidth="600.0"
      spacing="10.0"
      xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="lt.pauliusk.codetheory.controller.input.BinaryInputController">
   <children>
      <TextField fx:id="mBinaryInputTextArea"/>
      <HBox alignment="BOTTOM_RIGHT" prefHeight="50.0" prefWidth="580.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onBackButtonClicked" prefHeight="26.0" prefWidth="80.0" text="Back" />
            <Button mnemonicParsing="false" onAction="#onNextButtonClicked" prefHeight="26.0" prefWidth="80.0" text="Next" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
